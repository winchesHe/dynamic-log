{
  "name": "@winches/dynamic-log",
  "type": "module",
  "version": "0.0.0",
  "description": "dynamic log for your nodejs project enhance your log with color and emoji/ascii art",
  "author": "winches",
  "license": "MIT",
  "keywords": [
    "log",
    "dynamic-log",
    "nodejs",
    "typescript",
    "color",
    "emoji",
    "ascii"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "npm run build",
    "dev": "tsup --watch",
    "build": "tsup-node",
    "start": "tsx src/index.ts",
    "lint": "eslint --cache --ext ts,tsx,vue .",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s && git add .",
    "release": "bumpp --execute=\"nr changelog\" --all && npm publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.42.1",
    "@types/node": "^18.7.18",
    "bumpp": "^9.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.52.0",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "log-update": "^6.0.0"
  }
}
